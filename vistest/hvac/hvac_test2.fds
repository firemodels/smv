&INIT XB=0,0.5,0,1,0,1,MASS_FRACTION(1)=1,SPEC_ID(1)='MYSPEC2'/

&VENT XB=0,0,0.4,0.6,0.1,0.3,SURF_ID='HVAC',COLOR='RED',ID='I'/
